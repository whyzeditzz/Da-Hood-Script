-- Create the secondary GUI
local SecondaryGui = Instance.new("ScreenGui")
local SecondaryFrame = Instance.new("Frame")
local UICorner2 = Instance.new("UICorner")
local LockTitle = Instance.new("TextLabel")
local ToggleButton = Instance.new("TextButton")
local UICornerToggle = Instance.new("UICorner")
local CloseButton2 = Instance.new("TextButton")
local MinimizeButton2 = Instance.new("TextButton")
local MinimizedFrame2 = Instance.new("Frame")
local UICornerMinimized2 = Instance.new("UICorner")
local OpenButton2 = Instance.new("TextButton")

SecondaryGui.Name = "SecondaryGui"
SecondaryGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
SecondaryGui.Enabled = false

SecondaryFrame.Name = "SecondaryFrame"
SecondaryFrame.Parent = SecondaryGui
SecondaryFrame.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
SecondaryFrame.Position = UDim2.new(0.5, -100, 0.5, -50)
SecondaryFrame.Size = UDim2.new(0, 200, 0, 100)
SecondaryFrame.Active = true
