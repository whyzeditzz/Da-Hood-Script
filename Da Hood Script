CloseButton2.Position = UDim2.new(1, -30, 0, 0)
CloseButton2.Size = UDim2.new(0, 30, 0, 30)
CloseButton2.Font = Enum.Font.GothamBold
CloseButton2.Text = "X"
CloseButton2.TextColor3 = Color3.new(1, 1, 1)
