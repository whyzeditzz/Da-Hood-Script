MinimizedFrame2.Name = "MinimizedFrame"
MinimizedFrame2.Parent = SecondaryGui
MinimizedFrame2.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
MinimizedFrame2.Position = UDim2.new(0.5, -50, 0.5, -25)
MinimizedFrame2.Size = UDim2.new(0, 100, 0, 50)
MinimizedFrame2.Visible = false
MinimizedFrame2.Active = true
MinimizedFrame2.Draggable = true

UICornerMinimized2.CornerRadius = UDim.new(0, 10)
UICornerMinimized2.Parent = MinimizedFrame2

OpenButton2.Name = "OpenButton"
OpenButton2.Parent = MinimizedFrame2
OpenButton2.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
OpenButton2.Position = UDim2.new(0.5, -50, 0.5, -15)
OpenButton2.Size = UDim2.new(0, 100, 0, 30)
OpenButton2.Font = Enum.Font.Gotham
OpenButton2.Text = "Open"
OpenButton2.TextColor3 = Color3.new(1, 1, 1)
OpenButton2.TextSize = 14

-- Function to close the main GUI
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

-- Function to minimize the main GUI
MinimizeButton.MouseButton1Click:Connect(function()
    Frame.Visible = false
    MinimizedFrame.Visible = true
end)

-- Function to open the minimized main GUI
OpenButton.MouseButton1Click:Connect(function()
    Frame.Visible = true
    MinimizedFrame.Visible = false
end)

-- Function to close the secondary GUI
CloseButton2.MouseButton1Click:Connect(function()
    SecondaryGui:Destroy()
end)

-- Function to minimize the secondary GUI
MinimizeButton2.MouseButton1Click:Connect(function()
    SecondaryFrame.Visible = false
    MinimizedFrame2.Visible = true
end)

-- Function to open the minimized secondary GUI
OpenButton2.MouseButton1Click:Connect(function()
    SecondaryFrame.Visible = true
    MinimizedFrame2.Visible = false
end)

-- Function to toggle the secondary GUI
Button.MouseButton1Click:Connect(function()
    SecondaryGui.Enabled = not SecondaryGui.Enabled
end)

-- Function to lock the camera
local cameraLocked = false

ToggleButton.MouseButton1Click:Connect(function()
    cameraLocked = not cameraLocked
    if cameraLocked then
        ToggleButton.Text = "Unlock"
    else
        ToggleButton.Text = "Lock"
    end
end)

game:GetService("UserInputService").InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.C and cameraLocked then
        local player = game.Players.LocalPlayer
        local character = player.Character
        if character then
            local target = nil
            for _, v in pairs(game.Players:GetPlayers()) do
                if v ~= player and v.Character then
                    target = v.Character
                    break
                end
            end
            if target then
                local camera = game.Workspace.CurrentCamera
                camera.CameraSubject = target
            end
        end
    end
end)
